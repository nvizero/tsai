                    </tr>
                  </thead>



  <tbody>
    <% @pios.each do |pio| %>

    <%= render 'comm/info' , :info => pio , :users => @users_a , :title => "產品:#{pio.id} 訊息"  %>

      <tr>
        <td><%= pio.code %></td>
        <td><%= pio.serial %></td>
        <td><%= pio.num %></td>
        <td>
          <%# if pio.product.verify_type_main %>
            <%=pio.store_area_id %>
          <%# end %>
        </td>


        <td>
          <%# if pio.user %>
            <%= pio.save_date %>
          <%# end %>
        </td>

        <td>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>
